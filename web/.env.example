# E2E testing
REEARTH_CMS_E2E_USERNAME=
REEARTH_CMS_E2E_PASSWORD=

# IAP Authentication
# Toggle IAP authentication (auto-detects for dev/staging vs localhost/production if not set)
USE_IAP_AUTH=
# Authentication method: 'adc', 'service-account', or 'id-token'
IAP_AUTH_METHOD=
# OAuth client ID for the IAP-protected resource
IAP_TARGET_AUDIENCE=
# Service account credentials (required for service-account method)
GOOGLE_SERVICE_ACCOUNT_JSON=
# ID token (required for id-token method)
IAP_ID_TOKEN=

# SASS config
REEARTH_CMS_DISABLE_WORKSPACE_UI=
